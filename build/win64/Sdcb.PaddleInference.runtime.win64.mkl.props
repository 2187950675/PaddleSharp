<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
	<PropertyGroup>
		<NativeDlls>$(MSBuildThisFileDirectory)..\..\runtimes</NativeDlls>
	</PropertyGroup>
	<ItemGroup Condition="!$(TargetFramework.Contains('netstandard')) And !$(TargetFramework.Contains('netcoreapp')) And !$(TargetFramework.Contains('net5') And !$(TargetFramework.Contains('net6'))">
		<Content Include="$(NativeDlls)\win-x64\native\paddle_inference_c.dll">
			<Link>dll\x64\paddle_inference_c.dll</Link>
			<CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
		</Content>
		<Content Include="$(NativeDlls)\win-x64\native\mkldnn.dll">
			<Link>dll\x64\mkldnn.dll</Link>
			<CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
		</Content>
		<Content Include="$(NativeDlls)\win-x64\native\mklml.dll">
			<Link>dll\x64\mklml.dll</Link>
			<CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
		</Content>
		<Content Include="$(NativeDlls)\win-x64\native\libiomp5md.dll">
			<Link>dll\x64\libiomp5md.dll</Link>
			<CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
		</Content>
	</ItemGroup>
</Project>
